<div class="container">
  <div class="wrapper col-md-8 col-md-offset-2 col-sm-10">
<%= form_for(@contact, url: contact_confirm_contacts_path) do |f| %>



<%= f.label :name %>
<%= f.text_field :name, class: 'form-control' %>
<br>
<%= f.label :email %>
<%= f.text_field :email, class: 'form-control' %>
<br>

<%= f.label :content %>
<%= f.text_field :content, class: 'form-control' %>
<br>
<%= f.submit "作成" ,class: 'btn btn-primary btn-block'  %>

<% end %>

</div>
</div>
